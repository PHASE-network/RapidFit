<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.68</Value>
			<Type>Unbounded</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.068</Value>
			<Type>Unbounded</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.16</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.60</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>2.50</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>-0.17</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.77</Value>
			<Minimum>17.0</Minimum>
                        <Maximum>19.0</Maximum>
			<Type>Fixed</Type>
			<Unit>MeV/c^{2}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>mistag</Name>
			<Value>0.33</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>-0.7</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

                <PhysicsParameter>
                        <Name>timeRes</Name>
                        <Value>0.0</Value>
                        <Type>Fixed</Type>
                        <Unit>ps</Unit>
                </PhysicsParameter>

	</ParameterSet>

	// Minimising with Minuit2
	<Minimiser>Minuit2</Minimiser>

	// Requesting two contour plots, pull plots and time projection
	<Output>
	</Output>

	// Minimising weighted NLL
	<FitFunction>
		<FunctionName>NegativeLogLikelihood</FunctionName>
	</FitFunction>

	// Single fit
	<NumberRepeats>1</NumberRepeats>

	// 100000 toy events
	<ToFit>
		<PDF>
			<Name>Bs2JpsiPhi_mistagParameter</Name>
		</PDF>

		<DataSet>
			<Source>File</Source>
			<FileName>../../data/observables_MC09_Bs2JpsiPhi_lhcb_conventions.root</FileName>
			<NTuplePath>dataNTuple</NTuplePath>
			<NumberEvents>10000</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>15.0</Maximum>
					<Unit>ps</Unit>
				</Observable>

				<Observable>
					<Name>cosTheta</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>phi</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>cosPsi</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>tag</Name>
					<Value>0.0</Value>
					<Unit>Unitless</Unit>
				</Observable>

			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>
</RapidFit>
