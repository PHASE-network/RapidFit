<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.7</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>dgam</Name>
			<Value>0.06</Value>
			<Minimum>-1.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Rt</Name>
			<Value>0.16</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Rp</Name>
			<Value>0.24</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta1</Name>
			<Value>-2.67</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta2</Name>
			<Value>-0.17</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_ms</Name>
			<Value>17.77</Value>
			<Minimum>15.0</Minimum>
			<Maximum>25.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>phi_s</Name>
			<Value>-0.04</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>tagFraction</Name>
			<Value>0.0</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.5</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>resolution</Name>
			<Value>0.001</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.1</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
	</ParameterSet>

	<Minimiser>Minuit</Minimiser>

	<FitFunction>NegativeLogLikelihood</FitFunction>

	<ToFit>
		<PDF>
			<Name>RooJPsiPhiWrapper</Name>
		</PDF>
		<DataSet>
			<Source>AcceptReject</Source>
			<NumberEvents>10000</NumberEvents>
			<PhaseSpaceBoundary>
				<SingleBound>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>20.0</Maximum>
					<Unit>ms</Unit>
				</SingleBound>

				<SingleBound>
					<Name>ctheta_tr</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>rad</Unit>
				</SingleBound>

				<SingleBound>
					<Name>phi_tr</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</SingleBound>

				<SingleBound>
					<Name>ctheta_1</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>rad</Unit>
				</SingleBound>
			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>
</RapidFit>
