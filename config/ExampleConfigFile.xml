<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>sigma</Name>
			<Value>1.0</Value>
			<Minimum>0.5</Minimum>
			<Maximum>1.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>mean</Name>
			<Value>6.0</Value>
			<Minimum>5.5</Minimum>
			<Maximum>6.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>mean2</Name>
			<Value>2.0</Value>
			<Minimum>1.5</Minimum>
			<Maximum>2.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.7</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>dgam</Name>
			<Value>0.1</Value>
			<Minimum>-1.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Rt</Name>
			<Value>0.2</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Rp</Name>
			<Value>0.2</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta1</Name>
			<Value>-0.46</Value>
			<Minimum>-6.28</Minimum>
			<Maximum>6.28</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta2</Name>
			<Value>2.92</Value>
			<Minimum>-6.28</Minimum>
			<Maximum>6.28</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_ms</Name>
			<Value>17.77</Value>
			<Minimum>15.0</Minimum>
			<Maximum>25.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>phi_s</Name>
			<Value>-0.04</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>tagFraction</Name>
			<Value>0.33</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>resolution</Name>
			<Value>0.0</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
	</ParameterSet>

	<Minimiser>Minuit</Minimiser>

	<FitFunction>NegativeLogLikelihood</FitFunction>

	<ToFit>
		<SumPDF>
			<FractionName>tagFraction</FractionName>

			<PDF>
				<Name>RooGaussianWrapper</Name>
			</PDF>

			<PDF>
				<Name>RooGaussianWrapper</Name>
				<ObservableName>xValue</ObservableName>
				<ParameterName>mean2</ParameterName>
				<ParameterName>sigma</ParameterName>
			</PDF>
		</SumPDF>
		<DataSet>
			<Source>AcceptReject</Source>
			<NumberEvents>10</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>xValue</Name>
					<Minimum>0.0</Minimum>
					<Maximum>10.0</Maximum>
					<Unit>xLength</Unit>
				</Observable>
			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>

	<ToFit>
		<PDF>
			<Name>RooJPsiPhiWrapper</Name>
		</PDF>
		<DataSet>
			<Source>JPsiPhiDataGenerator</Source>
			<NumberEvents>10000</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>100.0</Maximum>
					<Unit>ms</Unit>
				</Observable>

				<Observable>
					<Name>ctheta_tr</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>phi_tr</Name>
					<Minimum>-3.14</Minimum>
					<Maximum>3.14</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>ctheta_1</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>rad</Unit>
				</Observable>
			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>
</RapidFit>
