<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.70</Value>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.084</Value>
			<Minimum>-1.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Unbounded</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.233</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.556</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>-2.93</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>2.91</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.77</Value>
			<Type>Fixed</Type>
			<Unit>MeV/c2</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>-0.0368</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>SignalFractionTagged</Name>
			<Value>0.40</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>LongLivedFractionTagged</Name>
			<Value>0.37</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>SignalFractionUntagged</Name>
			<Value>0.23</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>LongLivedFractionUntagged</Name>
			<Value>0.13</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>tau_LL1</Name>
			<Value>1.114</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>tau_LL2</Name>
			<Value>0.161</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>f_LL1</Name>
			<Value>0.22</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sigmaPr</Name>
			<Value>0.044</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>f_sig_m1</Name>
			<Value>0.74</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sigma_m1</Name>
			<Value>13.2</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>sigma_m2</Name>
			<Value>22.5</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>m_Bs</Name>
			<Value>5366.4</Value>
			<Type>Fixed</Type>
			<Unit>MeV/c2</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>alphaM_pr</Name>
			<Value>0.0006</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>


	</ParameterSet>



	<Minimiser>Minuit2</Minimiser>



	<FitFunction>NegativeLogLikelihood</FitFunction>



	<NumberRepeats>1</NumberRepeats>



	<ToFit>

        <NormalisedSumPDF>
            <FractionName>SignalFractionTagged</FractionName>
            <ProdPDF>
                <PDF>
                    <Name>RaPDF_Bs2JpsiPhiNew</Name>
                </PDF>
                <PDF>
                    <Name>RaPDF_Bs2JpsiPhiMassSignal</Name>
                </PDF>
            </ProdPDF>
            <NormalisedSumPDF>
                <FractionName>LongLivedFractionTagged</FractionName>
                <ProdPDF>
                    <PDF>
                        <Name>RaPDF_Bs2JpsiPhiLongLivedBkg</Name>
                    </PDF>
                    <PDF>
                        <Name>RaPDF_Bs2JpsiPhiMassBkg</Name>
                    </PDF>
                </ProdPDF>
                <ProdPDF>
                    <PDF>
                        <Name>Bs2JpsiPhiPromptBkg_withTimeRes</Name>
                    </PDF>
                    <PDF>
                        <Name>RaPDF_Bs2JpsiPhiMassBkg</Name>
                    </PDF>
                </ProdPDF>
            </NormalisedSumPDF>
        </NormalisedSumPDF>

		<DataSet>
			<Source>Foam</Source>
			<NumberEvents>166000</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>15.0</Maximum>
					<Unit>ps</Unit>
				</Observable>

				<Observable>
					<Name>cosTheta</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>phi</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>cosPsi</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>tag</Name>
					<Value>1.0</Value>
					<Value>-1.0</Value>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>mistag</Name>
					<Value>0.334</Value>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>mass</Name>
					<Minimum>5300.0</Minimum>
					<Maximum>5430.0</Maximum>
					<Unit>MeV/c2</Unit>
				</Observable>

			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>

	<ToFit>


        <NormalisedSumPDF>
            <FractionName>SignalFractionUntagged</FractionName>
            <ProdPDF>
                <PDF>
                    <Name>RaPDF_Bs2JpsiPhiNew</Name>
                </PDF>
                <PDF>
                    <Name>RaPDF_Bs2JpsiPhiMassSignal</Name>
                </PDF>
            </ProdPDF>
            <NormalisedSumPDF>
                <FractionName>LongLivedFractionUntagged</FractionName>
                <ProdPDF>
                    <PDF>
                        <Name>RaPDF_Bs2JpsiPhiLongLivedBkg</Name>
                    </PDF>
                    <PDF>
                        <Name>RaPDF_Bs2JpsiPhiMassBkg</Name>
                    </PDF>
                </ProdPDF>
                <ProdPDF>
                    <PDF>
                        <Name>Bs2JpsiPhiPromptBkg_withTimeRes</Name>
                    </PDF>
                    <PDF>
                        <Name>RaPDF_Bs2JpsiPhiMassBkg</Name>
                    </PDF>
                </ProdPDF>
            </NormalisedSumPDF>
        </NormalisedSumPDF>

		<DataSet>
			<Source>Foam</Source>
			<NumberEvents>221000</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>15.0</Maximum>
					<Unit>ps</Unit>
				</Observable>

				<Observable>
					<Name>cosTheta</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>phi</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>cosPsi</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>tag</Name>
					<Value>0.0</Value>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>mistag</Name>
					<Value>0.334</Value>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>mass</Name>
					<Minimum>5300.0</Minimum>
					<Maximum>5430.0</Maximum>
					<Unit>MeV/c2</Unit>
				</Observable>


			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>


</RapidFit>
