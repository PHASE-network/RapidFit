<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.68</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.06</Value>
			<Minimum>-1.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.16</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.60</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>2.50</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>-0.17</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.77</Value>
			<Minimum>17.0</Minimum>
                        <Maximum>19.0</Maximum>
			<Type>Fixed</Type>
			<Unit>MeV/c2</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>-0.7</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		
		<PhysicsParameter>
			<Name>mistag</Name>
			<Value>0.35</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>timeResolution1</Name>
			<Value>0.025</Value>
			<Type>Fixed</Type>
			<Unit>ps</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>timeResolution2</Name>
			<Value>0.069</Value>
			<Type>Fixed</Type>
			<Unit>ps</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>timeResolution1Fraction</Name>
			<Value>0.78</Value>
			<Type>Fixed</Type>
			<Unit> </Unit>
		</PhysicsParameter>

		//........... Signal mass PDF parameters ...........
<PhysicsParameter>
<Name>f_sig_m1</Name>
<Value>0.55</Value>
<Type>Fixed</Type>
<Unit>Unitless</Unit>
</PhysicsParameter>

<PhysicsParameter>
<Name>sigma_m1</Name>
<Value>11.0</Value>
<Type>Fixed</Type>
<Unit>Unitless</Unit>
</PhysicsParameter>

<PhysicsParameter>
<Name>sigma_m2</Name>
<Value>18.5</Value>
<Type>Fixed</Type>
<Unit>Unitless</Unit>
</PhysicsParameter>

<PhysicsParameter>
<Name>m_Bs</Name>
<Value>5364.4</Value>
<Type>Fixed</Type>
<Unit>MeV/c^{2}</Unit>
</PhysicsParameter>


		// 11.1701
		<PhysicsParameter>
			<Name>angAccI1</Name>
			<Value>10.58</Value>
			<Minimum>1.0</Minimum>
			<Maximum>15.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>angAccI2</Name>
			<Value>1.199</Value>
			<Minimum>1.0</Minimum>
			<Maximum>15.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>angAccI3</Name>
			<Value>1.324</Value>
			<Minimum>1.0</Minimum>
			<Maximum>15.0</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>angAccI4</Name>
			<Value>-0.0053</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>angAccI5</Name>
			<Value>0.0022</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>angAccI6</Name>
			<Value>-0.00071</Value>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
	</ParameterSet>

	// Minimising with Minuit2
	<Minimiser>Minuit2</Minimiser>

	// Requesting two contour plots, pull plots and time projection
	<Output>
		<Projection>time</Projection>
	</Output>

	// Minimising weighted NLL
	<FitFunction>
		<FunctionName>NegativeLogLikelihood</FunctionName>
	</FitFunction>

	// Single fit
	<NumberRepeats>1</NumberRepeats>

	// 100000 toy events
	<ToFit>
		<ProdPDF>
		<PDF>
			<Name>Bs2JpsiPhi_mistagParameter_withTimeRes_withAverageAngAcc</Name>
		</PDF>
		<PDF>
			<Name>Bs2JpsiPhiMassSignal</Name>
		</PDF>
		</ProdPDF>

		<DataSet>
			<Source>File</Source>
			<FileName>../../data/observables_MC09_Bs2JpsiPhi_lhcb_conventions_NP_phis.root</FileName>
			<NTuplePath>ntuple</NTuplePath>
			<NumberEvents>117000</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>time</Name>
					<Minimum>-0.5</Minimum>
					<Maximum>12.0</Maximum>
					<Unit>ps</Unit>
				</Observable>

				<Observable>
					<Name>cosTheta</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>phi</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>cosPsi</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>tag</Name>
					<Value>0.0</Value>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>mass</Name>
					<Minimum>5300.0</Minimum>
					<Maximum>5430.0</Maximum>
					<Unit>MeV/c^{2}</Unit>
				</Observable>
			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>
</RapidFit>
