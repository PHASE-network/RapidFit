<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.8</Value>
			<Minimum>17.0</Minimum>
			<Maximum>20.0</Maximum>
			<Type>Fixed</Type>
			<Unit>MeV/c2</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.68</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.5</Maximum>
			<Type>Fixed</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.05</Value>
			<Minimum>-1.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Fixed</Type>
			<Unit>ps^{-1}</Unit>
		</PhysicsParameter>
		
		<PhysicsParameter>
			<Name>delta_zero</Name>
			<Value>0.0</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		
		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>-2.93</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>2.91</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
		
		<PhysicsParameter>
			<Name>Azero_sq</Name>
			<Value>0.56</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.233</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>As_sq</Name>
			<Value>0.111</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_s</Name>
			<Value>1.5707963267948966</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>-0.5</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
	</ParameterSet>

	<Minimiser>Minuit2</Minimiser>

	<FitFunction>NegativeLogLikelihood</FitFunction>

	<NumberRepeats>5</NumberRepeats>
	<Output>
		        <DoPullPlots>FlatNTuple</DoPullPlots>
		</Output>



	<ToFit>

		<PDF>
			<Name>RaPDF_Bs2JpsiPhiNew</Name>
		</PDF>

		<DataSet>
		<PDF>
			<Name>RaPDF_Bs2JpsiPhi_sWave</Name>
		</PDF>

			<Source>Foam</Source>
			<NumberEvents>30000</NumberEvents>
			<PhaseSpaceBoundary>
				<Observable>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>15.0</Maximum>
					<Unit>ps</Unit>
				</Observable>

				<Observable>
					<Name>cosTheta</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>phi</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</Observable>

				<Observable>
					<Name>cosPsi</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>tag</Name>
					<Value>1.0</Value>
					<Value>0.0</Value>
					<Value>-1.0</Value>
					<Unit>Unitless</Unit>
				</Observable>

				<Observable>
					<Name>mistag</Name>
					<Value>0.33</Value>
					<Unit>Unitless</Unit>
				</Observable>
			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>
</RapidFit>
