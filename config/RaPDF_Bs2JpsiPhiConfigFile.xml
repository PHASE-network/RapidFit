<RapidFit>
	/////////////////////////////////////////
	// Example RapidFit configuration file //
	// Author: Ben Wynne                   //
	/////////////////////////////////////////
	<ParameterSet>
		<PhysicsParameter>
			<Name>gamma</Name>
			<Value>0.7</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.5</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaGamma</Name>
			<Value>0.06</Value>
			<Minimum>-1.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Aperp_sq</Name>
			<Value>0.16</Value>
			<Minimum>0.0</Minimum>
			<Maximum>1.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

                <PhysicsParameter>
                        <Name>Azero_sq</Name>
                        <Value>0.60</Value>
                        <Minimum>0.0</Minimum>
                        <Maximum>1.0</Maximum>
                        <Type>Free</Type>
                        <Unit>Unitless</Unit>
                </PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_para</Name>
			<Value>2.50</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>delta_perp</Name>
			<Value>-0.17</Value>
			<Minimum>-3.14159</Minimum>
			<Maximum>3.14159</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

	        <PhysicsParameter>
                        <Name>delta_zero</Name>
                        <Value>0.0</Value>
                        <Minimum>-3.14159</Minimum>
                        <Maximum>3.14159</Maximum>
                        <Type>Fixed</Type>
                        <Unit>Unitless</Unit>
                </PhysicsParameter>

		<PhysicsParameter>
			<Name>deltaM</Name>
			<Value>17.77</Value>
			<Minimum>15.0</Minimum>
			<Maximum>20.0</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>Phi_s</Name>
			<Value>-0.04</Value>
			<Minimum>-3.14</Minimum>
			<Maximum>3.14</Maximum>
			<Type>Free</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>tagFraction</Name>
			<Value>0.0</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.5</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>

		<PhysicsParameter>
			<Name>resolution</Name>
			<Value>0.0</Value>
			<Minimum>0.0</Minimum>
			<Maximum>0.1</Maximum>
			<Type>Fixed</Type>
			<Unit>Unitless</Unit>
		</PhysicsParameter>
	</ParameterSet>

	<Minimiser>Minuit</Minimiser>

	<FitFunction>NegativeLogLikelihood</FitFunction>

	<ToFit>
		<PDF>
			<Name>Bs2JpsiPhi</Name>
		</PDF>
		<DataSet>
			<Source>File</Source>
			<FileName>data_newEvtGen_100k_lhcb_convention.txt</FileName>
			<NumberEvents>100000</NumberEvents>
			<PhaseSpaceBoundary>
				<SingleBound>
					<Name>time</Name>
					<Minimum>0.0</Minimum>
					<Maximum>16.0</Maximum>
					<Unit>ms</Unit>
				</SingleBound>

				<SingleBound>
					<Name>cosTheta</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</SingleBound>

				<SingleBound>
					<Name>phi</Name>
					<Minimum>-3.14159</Minimum>
					<Maximum>3.14159</Maximum>
					<Unit>rad</Unit>
				</SingleBound>

				<SingleBound>
					<Name>cosPsi</Name>
					<Minimum>-1.0</Minimum>
					<Maximum>1.0</Maximum>
					<Unit>Unitless</Unit>
				</SingleBound>
                                <SingleBound>
                                        <Name>tag</Name>
                                        <Minimum>-1.0</Minimum>
                                        <Maximum>1.0</Maximum>
                                        <Unit>Unitless</Unit>
                                </SingleBound>
			</PhaseSpaceBoundary>
		</DataSet>
	</ToFit>
</RapidFit>
